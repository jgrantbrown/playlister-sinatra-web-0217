<h1> Details about the Genre </h1>

<h2> <%= @genre.name %> <h2>
  <h1> Song in this Genre </h1>
<ol>
  <% @genre.songs.each do |song| %>
    <li>
      <a href=/songs/<%= song.slug %>><%= song.name %></a>
       by <a href=/artists/<%= song.artist.slug %>><%= song.artist.name %>
    </li>
  <% end %>
</ol>
