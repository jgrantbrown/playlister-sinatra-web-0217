<h1> Details about the Artist </h1>

<h2> <%= @artist.name %> <h2>
  <h1> Songs </h1>
<ol>
  <% @artist.songs.each do |song| %>
    <li>  <label>Song name:</label>
            <a href=/songs/<%= song.slug %>>
              <%= song.name %>
            </a>

          <label> Genre:</label>
            <% song.genres.each do |genre| %>
              <a href=/genres/<%= genre.slug %>>
                  <%= genre.name %>
              </a>
            <% end %>

      </li>
  <% end %>
  </ol>
